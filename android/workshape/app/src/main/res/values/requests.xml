<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="request_product">select * from product_view where qr_code=\"#qr_code\"</string>
    <string name="request_product_out">call product_out(\"#qr_code\",\"#date\")</string>
    <string name="request_product_in">call product_in(\"#qr_code\",\"#date\",\"#longueur_consommee\",\"#temps_hors_gel\")</string>
    <string name="request_product_add">call product_add(\"#qr_code\",\"#date\",\"#reference\",\"#fournisseur\",\"#ref_fournisseur\",\"#longueur_initiale\",\"#largeur\",\"#grammage\",\"#type_de_tissus\",\"#date_arrivee\",\"#transport_frigo\",\"#lieu_actuel\")</string>
    <string name="request_product_update">call product_update(\"#qr_code\",\"#date\",\"#reference\",\"#fournisseur\",\"#ref_fournisseur\",\"#longueur_initiale\",\"#largeur\",\"#grammage\",\"#type_de_tissus\",\"#date_arrivee\",\"#transport_frigo\",\"#lieu_actuel\")</string>
    <string name="request_product_update_note">call product_update_note(\"#qr_code\",\"#date\",\"#note\")</string>
    <string name="date_format_to_mysql">dd/MM/yyyy HH:mm:ss</string>
    <string name="date_format_to_android">dd/MM/yyyy HH:mm:ss</string>
    <string name="time_format_to_android">HH:mm</string>
    <string name="lieu_de_stockage">frigo</string>
    <string name="lieu_de_sortie">sortie</string>
</resources>
